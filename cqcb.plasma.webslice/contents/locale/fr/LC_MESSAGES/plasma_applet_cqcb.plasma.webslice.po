msgid "Back"
msgstr "Précédent"

#: ../ui/main.qml:193
msgid "Forward"
msgstr "Suivant"

#: ../ui/main.qml:200
msgid "Reload"
msgstr "Recharger"

#: ../ui/main.qml:207
msgid "Go to"
msgstr "Aller à"

#: ../ui/main.qml:228
msgid "Open current URL in default browser"
msgstr "Ouvrir l'URL actuelle dans le navigateur par défaut"

